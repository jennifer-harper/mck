{% set singles = craft.entries.section('singles').all() %}
{% set links = [
    { 'url': siteUrl, 'label': 'Home' },
    { 'url': '/covers', 'label': 'Covers' },
    { 'url': '/branding', 'label': 'Branding' }
] %}

<nav class="flex flex-row items-center justify-between w-full p-[20px] bg-gray-600">
    <a class="p-[10px]" href="{{siteUrl}}">
        <svg x="0px" y="0px" viewBox="0 0 126.08 124.77" width="60px" height="60px">
            <circle cx="63" cy="62" r="60"/>
        </svg>
    </a>

<div class="flex flex-row">
    {% for single in singles %}
        {% set links = links | merge([ { 'url': single.url, 'label': single.title } ]) %}
    {% endfor %}

    {% for link in links %}
        <a class="p-[10px] uppercase text-white hover:text-gray-400" href="{{ link.url }}">{{ link.label }}</a>
    {% endfor %}
</div>
</nav>



